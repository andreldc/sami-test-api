{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "lib": [
      "es5",
      "es6",
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true    
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
