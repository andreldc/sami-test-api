    "eslint-plugin-promise": "5.0.0",
    "fastify-tsconfig": "^1.0.1",
    "git-commit-msg-linter": "^3.2.8",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.0.0",
    "typescript": "4"
  }
}
