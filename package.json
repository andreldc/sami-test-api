    "cpf-cnpj-validator": "^1.0.3",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "mysql2": "^2.3.3-rc.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
